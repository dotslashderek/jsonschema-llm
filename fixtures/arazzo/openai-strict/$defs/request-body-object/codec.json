{
  "$schema": "https://jsonschema-llm.dev/codec/v1",
  "transforms": [
    {
      "type": "nullable_optional",
      "path": "#/properties/contentType",
      "originalRequired": false
    },
    {
      "type": "nullable_optional",
      "path": "#/properties/payload",
      "originalRequired": false
    },
    {
      "type": "nullable_optional",
      "path": "#/properties/replacements",
      "originalRequired": false
    },
    {
      "type": "json_string_parse",
      "path": "#/properties/payload/anyOf/0"
    },
    {
      "type": "json_string_parse",
      "path": "#/properties/replacements/anyOf/0/items/patternProperties/^x-"
    },
    {
      "type": "json_string_parse",
      "path": "#/patternProperties/^x-"
    }
  ],
  "droppedConstraints": [
    {
      "path": "#/properties/replacements/anyOf/0",
      "constraint": "uniqueItems",
      "value": true
    }
  ]
}
