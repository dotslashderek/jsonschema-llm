{
  "$defs": {
    "example": {
      "$comment": "https://spec.openapis.org/oas/v3.1.0#example-object",
      "$ref": "#/$defs/specification-extensions",
      "not": {
        "required": [
          "value",
          "externalValue"
        ]
      },
      "properties": {
        "description": {
          "type": "string"
        },
        "externalValue": {
          "format": "uri",
          "type": "string"
        },
        "summary": {
          "type": "string"
        },
        "value": true
      },
      "type": "object",
      "unevaluatedProperties": false
    },
    "example-or-reference": {
      "else": {
        "$ref": "#/$defs/example"
      },
      "if": {
        "required": [
          "$ref"
        ],
        "type": "object"
      },
      "then": {
        "$ref": "#/$defs/reference"
      }
    },
    "reference": {
      "$comment": "https://spec.openapis.org/oas/v3.1.0#reference-object",
      "properties": {
        "$ref": {
          "format": "uri-reference",
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "summary": {
          "type": "string"
        }
      },
      "type": "object",
      "unevaluatedProperties": false
    },
    "specification-extensions": {
      "$comment": "https://spec.openapis.org/oas/v3.1.0#specification-extensions",
      "patternProperties": {
        "^x-": true
      }
    }
  },
  "properties": {
    "example": true,
    "examples": {
      "additionalProperties": {
        "$ref": "#/$defs/example-or-reference"
      },
      "type": "object"
    }
  }
}
