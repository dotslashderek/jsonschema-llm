{
  "$schema": "https://json-schema-llm.dev/codec/v1",
  "transforms": [
    {
      "type": "nullable_optional",
      "path": "#/properties/description",
      "originalRequired": false
    },
    {
      "type": "nullable_optional",
      "path": "#/properties/externalValue",
      "originalRequired": false
    },
    {
      "type": "nullable_optional",
      "path": "#/properties/summary",
      "originalRequired": false
    },
    {
      "type": "nullable_optional",
      "path": "#/properties/value",
      "originalRequired": false
    },
    {
      "type": "json_string_parse",
      "path": "#/properties/value/anyOf/0"
    }
  ],
  "droppedConstraints": [
    {
      "path": "#",
      "constraint": "not",
      "value": {
        "required": [
          "value",
          "externalValue"
        ]
      }
    },
    {
      "path": "#/properties/externalValue/anyOf/0",
      "constraint": "format",
      "value": "uri"
    }
  ]
}
