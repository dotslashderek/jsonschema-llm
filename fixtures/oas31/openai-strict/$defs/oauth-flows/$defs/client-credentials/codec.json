{
  "$schema": "https://json-schema-llm.dev/codec/v1",
  "transforms": [
    {
      "type": "map_to_array",
      "path": "#/$defs/map-of-strings",
      "keyField": "key"
    },
    {
      "type": "nullable_optional",
      "path": "#/properties/refreshUrl",
      "originalRequired": false
    },
    {
      "type": "json_string_parse",
      "path": "#/patternProperties/^x-"
    }
  ],
  "droppedConstraints": [
    {
      "path": "#/properties/refreshUrl/anyOf/0",
      "constraint": "format",
      "value": "uri"
    },
    {
      "path": "#/properties/tokenUrl",
      "constraint": "format",
      "value": "uri"
    }
  ]
}
