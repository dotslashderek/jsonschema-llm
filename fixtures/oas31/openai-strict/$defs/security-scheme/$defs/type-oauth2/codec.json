        "additionalProperties": false,
        "properties": {
          "flows": {
            "description": "MUST be a valid JSON object serialized as a string, e.g. \"{\\\"key\\\": \\\"value\\\"}\". Do NOT output plain text â€” the value must parse with JSON.parse().",
