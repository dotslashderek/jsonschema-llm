{
  "$defs": {
    "APIKeyHTTPSecurityScheme": {
      "additionalProperties": false,
      "examples": [
        {
          "in": "header",
          "name": "api_key",
          "type": "httpApiKey"
        }
      ],
      "patternProperties": {
        "^x-[\\w\\d\\.\\x2d_]+$": {
          "$ref": "#/$defs/specificationExtension"
        }
      },
      "properties": {
        "description": {
          "description": "A short description for security scheme.",
          "type": "string"
        },
        "in": {
          "description": "The location of the API key. ",
          "enum": [
            "header",
            "query",
            "cookie"
          ],
          "type": "string"
        },
        "name": {
          "description": "The name of the header, query or cookie parameter to be used.",
          "type": "string"
        },
        "type": {
          "description": "The type of the security scheme. ",
          "enum": [
            "httpApiKey"
          ],
          "type": "string"
        }
      },
      "required": [
        "type",
        "name",
        "in"
      ],
      "type": "object"
    },
    "BearerHTTPSecurityScheme": {
      "additionalProperties": false,
      "patternProperties": {
        "^x-[\\w\\d\\.\\x2d_]+$": {
          "$ref": "#/$defs/specificationExtension"
        }
      },
      "properties": {
        "bearerFormat": {
          "description": "A hint to the client to identify how the bearer token is formatted.",
          "type": "string"
        },
        "description": {
          "description": "A short description for security scheme.",
          "type": "string"
        },
        "scheme": {
          "description": "The name of the HTTP Authorization scheme to be used in the Authorization header as defined in RFC7235.",
          "enum": [
            "bearer"
          ],
          "type": "string"
        },
        "type": {
          "description": "The type of the security scheme. ",
          "enum": [
            "http"
          ],
          "type": "string"
        }
      },
      "required": [
        "type",
        "scheme"
      ],
      "type": "object"
    },
    "NonBearerHTTPSecurityScheme": {
      "additionalProperties": false,
      "not": {
        "properties": {
          "scheme": {
            "description": "A short description for security scheme.",
            "enum": [
              "bearer"
            ],
            "type": "string"
          }
        },
        "type": "object"
      },
      "patternProperties": {
        "^x-[\\w\\d\\.\\x2d_]+$": {
          "$ref": "#/$defs/specificationExtension"
        }
      },
      "properties": {
        "description": {
          "description": "A short description for security scheme.",
          "type": "string"
        },
        "scheme": {
          "description": "The name of the HTTP Authorization scheme to be used in the Authorization header as defined in RFC7235.",
          "type": "string"
        },
        "type": {
          "description": "The type of the security scheme. ",
          "enum": [
            "http"
          ],
          "type": "string"
        }
      },
      "required": [
        "scheme",
        "type"
      ],
      "type": "object"
    },
    "specificationExtension": {
      "additionalItems": true,
      "additionalProperties": true,
      "description": "Any property starting with x- is valid."
    }
  },
  "oneOf": [
    {
      "$ref": "#/$defs/NonBearerHTTPSecurityScheme"
    },
    {
      "$ref": "#/$defs/BearerHTTPSecurityScheme"
    },
    {
      "$ref": "#/$defs/APIKeyHTTPSecurityScheme"
    }
  ]
}
