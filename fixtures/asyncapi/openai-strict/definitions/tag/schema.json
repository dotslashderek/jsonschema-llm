{
  "additionalProperties": false,
  "description": "Allows adding meta data to a single tag.",
  "properties": {
    "description": {
      "anyOf": [
        {
          "description": "A short description for the tag.",
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "externalDocs": {
      "anyOf": [
        {
          "additionalProperties": false,
          "description": "Additional external documentation for this tag.",
          "properties": {
            "description": {
              "anyOf": [
                {
                  "description": "A short description of the target documentation.",
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ]
            },
            "url": {
              "description": "The URL for the target documentation. This MUST be in the form of an absolute URL. Constraints: format: uri.",
              "type": "string"
            }
          },
          "required": [
            "description",
            "url"
          ],
          "type": "object"
        },
        {
          "type": "null"
        }
      ]
    },
    "name": {
      "description": "The name of the tag.",
      "type": "string"
    }
  },
  "required": [
    "description",
    "externalDocs",
    "name"
  ],
  "type": "object"
}
